{
    "compilerOptions": {
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "allowJs": true,
        "lib": ["ES2015"]
    },
    "include": ["src"]
}